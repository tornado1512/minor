<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0"
  metadata-complete="true">

  <display-name>Welcome to Tomcat</display-name>
  <description>
     Welcome to Tomcat
  </description>
<servlet>
	<servlet-name>app001</servlet-name>
	<servlet-class>controllers.UserRegisterServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>app001</servlet-name>
	<url-pattern>/user_register.do</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>app002</servlet-name>
	<servlet-class>controllers.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>app002</servlet-name>
	<url-pattern>/login.do</url-pattern>
</servlet-mapping>


  <servlet>
  <servlet-name>app003</servlet-name>
  <servlet-class>controllers.RestaurantServlet</servlet-class>
  </servlet>

  <servlet-mapping>
  <servlet-name>app003</servlet-name>
  <url-pattern>/Restaurant.do</url-pattern>
  </servlet-mapping>



  <servlet>
  <servlet-name>app004</servlet-name>
  <servlet-class>controllers.OwnerRegisterServlet</servlet-class>
  </servlet>

  <servlet-mapping>
  <servlet-name>app004</servlet-name>
  <url-pattern>/owner_register.do</url-pattern>
  </servlet-mapping>

   <servlet>
  <servlet-name>app005</servlet-name>
  <servlet-class>controllers.OwnerLoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
  <servlet-name>app005</servlet-name>
  <url-pattern>/owner_login.do</url-pattern>
  </servlet-mapping>




</web-app>